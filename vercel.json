{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9",
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}
